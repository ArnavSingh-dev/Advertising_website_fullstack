generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Quote {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  service   String
  message   String?
  userAgent String?   // optional
  createdAt DateTime @default(now())
}
